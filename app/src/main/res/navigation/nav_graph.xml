<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <activity
        android:id="@+id/startActivity"
        android:name="com.learnitevekri.ui.activities.StartActivity"
        android:label="activity_start"
        tools:layout="@layout/activity_start" />

    <activity
        android:id="@+id/mainActivity"
        android:name="com.learnitevekri.ui.activities.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />

    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.learnitevekri.ui.feature.settings.fragment.SettingsFragment"
        android:label="SettingsFragment">

        <action
            android:id="@+id/action_SettingsFragment_to_LoginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.learnitevekri.ui.feature.home.fragment.HomeFragment"
        android:label="HomeFragment">
        <action
            android:id="@+id/action_homeFragment_to_chaptersFragment"
            app:destination="@id/chaptersFragment" />
        <action
            android:id="@+id/action_homeFragment_to_chaptersFragment2"
            app:destination="@id/chaptersFragment" />
        <action
            android:id="@+id/action_home_to_courses"
            app:destination="@id/coursesFragment" />
        <action
            android:id="@+id/action_home_to_editProfile"
            app:destination="@id/editProfileFragment" />
    </fragment>

    <fragment
        android:id="@+id/notificationsFragment"
        android:name="com.learnitevekri.ui.feature.notifications.fragment.NotificationsFragment"
        android:label="NotificationsFragment">
        <action
            android:id="@+id/action_notificationsFragment_to_coursesFragment"
            app:destination="@id/coursesFragment" />
        <action
            android:id="@+id/action_notificationsFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.learnitevekri.ui.feature.login.fragment.LoginFragment"
        android:label="LoginFragment">
        <action
            android:id="@+id/action_loginFragment_to_registerFragment2"
            app:destination="@id/registerFragment" />
    </fragment>

    <fragment
        android:id="@+id/registerFragment"
        android:name="com.learnitevekri.ui.feature.register.fragment.RegisterFragment"
        android:label="RegisterFragment">
        <action
            android:id="@+id/action_registerFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>

    <fragment
        android:id="@+id/coursesFragment"
        android:name="com.learnitevekri.ui.feature.courses.courses.fragment.CoursesFragment"
        android:label="CoursesFragment">
        <action
            android:id="@+id/action_CoursesFragment_to_ChaptersFragment"
            app:destination="@id/chaptersFragment" />
        <action
            android:id="@+id/action_coursesFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_coursesFragment_to_teacherRequestFragment"
            app:destination="@id/teacherRequestFragment" />
        <action
            android:id="@+id/action_coursesFragment_to_addNewCourseFragment"
            app:destination="@id/addNewCourseFragment" />
        <action
            android:id="@+id/action_CoursesFragment_to_EditCourseFragment"
            app:destination="@id/editCourseFragment" />
        <action
            android:id="@+id/action_coursesFragment_to_addContentFragment"
            app:destination="@id/addContentFragment" />
    </fragment>

    <fragment
        android:id="@+id/editCourseFragment"
        android:name="com.learnitevekri.ui.feature.courses.courses.fragment.EditCourseFragment"
        android:label="EditCourseFragment">
        <action
            android:id="@+id/action_EditCourseFragment_to_CoursesFragment"
            app:destination="@id/coursesFragment" />
    </fragment>

    <fragment
        android:id="@+id/editChapterFragment"
        android:name="com.learnitevekri.ui.feature.courses.courses.fragment.EditChapterFragment"
        android:label="EditChapterFragment">
        <action
            android:id="@+id/action_EditChapterFragment_to_ChaptersFragment"
            app:destination="@id/chaptersFragment" />
    </fragment>

    <fragment
        android:id="@+id/editLessonFragment"
        android:name="com.learnitevekri.ui.feature.courses.courses.fragment.EditLessonFragment"
        android:label="EditLessonFragment">
        <action
            android:id="@+id/action_coursesFragment_to_addContentFragment"
            app:destination="@id/addContentFragment" />
        <action
            android:id="@+id/action_coursesFragment_to_addQuestionFragment"
            app:destination="@id/addQuestionFragment" />
    </fragment>

    <fragment
        android:id="@+id/teacherRequestFragment"
        android:name="com.learnitevekri.ui.feature.courses.courses.fragment.TeacherRequestFragment"
        android:label="TeacherRequestFragment">
        <action
            android:id="@+id/action_teacherRequestFragment_to_coursesFragment"
            app:destination="@id/coursesFragment" />
    </fragment>

    <fragment
        android:id="@+id/addNewCourseFragment"
        android:name="com.learnitevekri.ui.feature.courses.courses.fragment.AddNewCourseFragment"
        android:label="AddNewCourseFragment">
        <action
            android:id="@+id/action_addNewCourseFragment_to_addNewChapterFragment"
            app:destination="@id/addNewChapterFragment" />
    </fragment>

    <fragment
        android:id="@+id/addNewChapterFragment"
        android:name="com.learnitevekri.ui.feature.courses.courses.fragment.AddNewChapterFragment"
        android:label="AddNewChapterFragment">
        <action
            android:id="@+id/action_addNewChapterFragment_to_addNewLessonFragment"
            app:destination="@id/addNewLessonFragment" />
    </fragment>

    <fragment
        android:id="@+id/addNewLessonFragment"
        android:name="com.learnitevekri.ui.feature.courses.courses.fragment.AddNewLessonFragment"
        android:label="AddNewLessonFragment">
        <action
            android:id="@+id/action_addNewLessonFragment_to_addNewChapterFragment"
            app:destination="@id/addNewChapterFragment" />
        <action
            android:id="@+id/action_addNewLessonFragment_to_addContentFragment"
            app:destination="@id/addContentFragment" />
    </fragment>

    <fragment
        android:id="@+id/chaptersFragment"
        android:name="com.learnitevekri.ui.feature.courses.courses.fragment.ChaptersFragment"
        android:label="ChaptersFragment"
        tools:layout="@layout/fragment_chapters">
        <action
            android:id="@+id/action_chaptersFragment_to_quizFragment"
            app:destination="@id/quizFragment" />
        <action
            android:id="@+id/action_chaptersFragment_to_theoryFragment"
            app:destination="@id/theoryFragment" />
        <action
            android:id="@+id/action_chaptersFragment_to_courseFragment"
            app:destination="@id/coursesFragment" />
        <action
            android:id="@+id/action_chaptersFragment_to_userAnswerFragment"
            app:destination="@id/userAnswerFragment" />
        <action
            android:id="@+id/action_chaptersFragment_to_editChapterFragment"
            app:destination="@id/editChapterFragment" />
        <action
            android:id="@+id/action_chaptersFragment_to_editLessonFragment"
            app:destination="@id/editLessonFragment" />
        <action
            android:id="@+id/action_chaptersFragment_to_MoreLessonFragment"
            app:destination="@id/moreLessonFragment" />
    </fragment>

    <fragment
        android:id="@+id/moreLessonFragment"
        android:name="com.learnitevekri.ui.feature.courses.courses.fragment.MoreLessonFragment"
        android:label="MoreLessonFragment"
        tools:layout="@layout/fragment_more_lesson">
        <action
            android:id="@+id/action_moreLessonFragment_to_addQuestionFragment"
            app:destination="@id/addQuestionFragment" />
    </fragment>

    <fragment
        android:id="@+id/trueFalseQuizFragment"
        android:name="com.learnitevekri.ui.feature.courses.quiz.fragment.TrueFalseQuizFragment"
        android:label="TrueFalseQuizFragment"
        tools:layout="@layout/fragment_quiz_true_false" />

    <fragment
        android:id="@+id/multipleChoiceFragment"
        android:name="com.learnitevekri.ui.feature.courses.quiz.fragment.MultipleChoiceQuizFragment"
        android:label="MultipleChoiceFragment"
        tools:layout="@layout/fragment_quiz_multiple_choice" />

    <fragment
        android:id="@+id/quizFragment"
        android:name="com.learnitevekri.ui.feature.courses.quiz.fragment.QuizFragment"
        android:label="QuizFragment"
        tools:layout="@layout/fragment_quiz">

        <action
            android:id="@+id/action_quizFragment_to_TheoryFragment"
            app:destination="@id/theoryFragment" />
        <action
            android:id="@+id/action_quizFragment_to_chaptersFragment"
            app:destination="@id/chaptersFragment" />

    </fragment>
    <fragment
        android:id="@+id/theoryFragment"
        android:name="com.learnitevekri.ui.feature.courses.courses.fragment.TheoryFragment"
        android:label="TheoryFragment"
        tools:layout="@layout/fragment_theory" >
        <action
            android:id="@+id/action_theoryFragment_to_simpleAddContentFragment"
            app:destination="@id/simpleAddContentFragment" />
        <action
            android:id="@+id/action_theoryFragment_to_simpleEditContentFragment"
            app:destination="@id/simpleEditContentFragment" />
    </fragment>

    <fragment
        android:id="@+id/userAnswerFragment"
        android:name="com.learnitevekri.ui.feature.courses.courses.fragment.UserAnswersFragment"
        android:label="UserAnswerFragment"
        tools:layout="@layout/fragment_user_answers" />

    <fragment
        android:id="@+id/noInternetFragment"
        android:name="com.learnitevekri.ui.feature.splash.fragment.NoInternetFragment"
        android:label="NoInternetFragment"
        tools:layout="@layout/fragment_no_internet" />

    <fragment
        android:id="@+id/noConnectionFragment"
        android:name="com.learnitevekri.ui.feature.splash.fragment.NoConnectionFragment"
        android:label="NoConnectionFragment"
        tools:layout="@layout/fragment_no_connection" />

    <fragment
        android:id="@+id/serverErrorFragment"
        android:name="com.learnitevekri.ui.feature.splash.fragment.ServerErrorFragment"
        android:label="ServerErrorFragment"
        tools:layout="@layout/fragment_server_error" />

    <fragment
        android:id="@+id/editProfileFragment"
        android:name="com.learnitevekri.ui.feature.home.fragment.EditProfileFragment"
        android:label="EditProfileFragment"
        tools:layout="@layout/fragment_edit_profile">
        <action
            android:id="@+id/action_editProfile_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>

    <fragment
        android:id="@+id/quizMatchingFragment"
        android:name="com.learnitevekri.ui.feature.courses.quiz.fragment.MatchingQuizFragment"
        android:label="MatchingQuizFragment"
        tools:layout="@layout/fragment_quiz_matching" />

    <fragment
        android:id="@+id/adminFragment"
        android:name="com.learnitevekri.ui.feature.admin.fragment.AdminFragment"
        android:label="AdminFragment"
        tools:layout="@layout/fragment_admin" />

    <fragment
        android:id="@+id/addContentFragment"
        android:name="com.learnitevekri.ui.feature.courses.courses.fragment.AddContentFragment"
        android:label="AddContentFragment"
        tools:layout="@layout/fragment_add_content">
        <action
            android:id="@+id/action_addContentFragment_to_addQuestionFragment"
            app:destination="@id/addQuestionFragment" />
    </fragment>

    <fragment
        android:id="@+id/addQuestionFragment"
        android:name="com.learnitevekri.ui.feature.courses.quiz.fragment.AddQuestionFragment"
        android:label="AddQuestionFragment"
        tools:layout="@layout/fragment_add_question">
        <action
            android:id="@+id/action_addQuestionFragment_to_coursesFragment"
            app:destination="@id/coursesFragment" />
    </fragment>

    <fragment
        android:id="@+id/simpleAddContentFragment"
        android:name="com.learnitevekri.ui.feature.courses.courses.fragment.SimpleAddContentFragment"
        android:label="SimpleAddContentFragment"
        tools:layout="@layout/fragment_simple_add_content"/>

    <fragment android:id="@+id/simpleEditContentFragment"
        android:name="com.learnitevekri.ui.feature.courses.courses.fragment.SimpleEditContentFragment"
        android:label="SimpleEditContentFragment"
        tools:layout="@layout/fragment_simple_edit_content"/>


</navigation>